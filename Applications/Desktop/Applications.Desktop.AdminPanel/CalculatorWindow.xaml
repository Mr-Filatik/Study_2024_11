<Window x:Class="Applications.Desktop.AdminPanel.CalculatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Applications.Desktop.AdminPanel"
        mc:Ignorable="d"
        Title="CalculatorWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="mainField" IsEnabled="False">2+4*2-5</TextBox>
        <Button Click="Calculate">=</Button>
    </StackPanel>
</Window>
